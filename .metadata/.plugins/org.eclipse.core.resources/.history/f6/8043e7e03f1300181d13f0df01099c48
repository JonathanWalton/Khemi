package mithrandirr.khemi.api.mortar;

import net.minecraft.item.ItemStack;

public class GrindablePair {
	
	ItemStack itemStackIn;
	ItemStack itemStackOut;
	
	public GrindablePair(ItemStack itemStackIn, ItemStack itemStackOut) {
		this.itemStackIn = itemStackIn;
		this.itemStackOut = itemStackOut;
	}
	
	public ItemStack getItemStackIn() {
		return itemStackIn;
	}
	
	public ItemStack getItemStackOut() {
		return itemStackOut;
	}

}
